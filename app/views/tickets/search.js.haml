- case params[:target]
- when "ticket" then
  $("#ajax_area").html("#{escape_javascript(render(partial: "listing_tickets", locals: {tickets: @tickets, title: @title}))}");
- when "ticket_bot" then
  $("#tweetbot_area").html("#{escape_javascript(render(partial: "listing_tickets_bot", locals: {tickets: @tickets, title: t('ticket_bot_index')}))}");
